{
  "listeners": {
    "*:8080": {
      "pass": "routes"
    }
  },
  "routes": [
    {
      "action": {
        "share": "/var/www/html/public$uri",
        "fallback": {
          "pass": "applications/laravel"
        }
      }
    }
  ],
  "applications": {
    "laravel": {
      "type": "php",
      "root": "/var/www/html/public",
      "script": "index.php",
      "user": "root",
      "group": "root",
      "options": {
        "admin": {}
      }
    }
  }
}